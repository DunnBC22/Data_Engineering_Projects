        <entry key="script" type="xstring" value="import knime.scripting.io as knio%%00010import pandas as pd%%00010from datetime import datetime%%00010%%00010# Input Table%%00010input_table = knio.input_tables[0]%%00010%%00010# Convert table to Pandas DataFrame%%00010df = input_table.to_pandas()%%00010%%00010# Function to process each individual record%%00010def process_date(date_str):%%00010    if isinstance(date_str, str):%%00010        # Case 1: Strings like &quot;Date Not Captured&quot;%%00010        if date_str.strip().lower() == &quot;date not captured&quot;:%%00010            return datetime(1900, 1, 1)%%00010        # Case 2: Dates in the format '16-Feb-07'%%00010        else:%%00010            try:%%00010                return datetime.strptime(date_str, '%d-%b-%y')%%00010            except ValueError:%%00010                pass%%00010            # Case 3: Dates in the format '4/12/07' or '04/12/07'%%00010            try:%%00010                return datetime.strptime(date_str, '%m/%d/%y')%%00010            except ValueError:%%00010                pass%%00010    # If none of the above, return default value%%00010    return datetime(1900, 1, 1)%%00010%%00010# List of target columns%%00010target_columns = [%%00010    'po_sent_to_vendor_date',%%00010    'scheduled_delivery_date',%%00010    'delivered_to_client_date',%%00010    'delivery_recorded_date'%%00010]%%00010%%00010# Apply the function to each target column%%00010for column in target_columns:%%00010    if column in df.columns:%%00010        df[column] = df[column].apply(process_date)%%00010        # Convert dates to the desired format 'MM/DD/YYYY'%%00010        df[column] = df[column].dt.strftime('%m/%d/%Y')%%00010%%00010# Output Table%%00010knio.output_tables[0] = knio.Table.from_pandas(df)"/>
